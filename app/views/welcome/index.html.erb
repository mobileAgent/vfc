<% title(@page_title || t(:messages)) %>
<% if @speaker && @place.nil? %>
  <%= render "shared/speaker_bio" %>
<% elsif @speaker.nil? && @place %>
  <%= render "shared/place_bio" %>
<% else %>  
  <%= render "shared/motm" %>
  <%= render "shared/tag_cloud" %>
<% end %>
<%= render "shared/item_list" %>
